<body>
  <h1>Vailable Orders</h1>
</body>

<div class="row">
  <% @orders.each_slice(2) do |two_orders| %>
   
    <div class="row">
        <% two_orders.each do |order| %>
          <div class="col-md-4">
          <div class="well well-lg">

          <h2><%= link_to order.user_id, "/orders/#{order.id}" %></h2>

          <b>Quanty:</b><%= order.quanty %>
            
          </div>
        <% end %>
    </div>
  
  <% end %>
</div>
